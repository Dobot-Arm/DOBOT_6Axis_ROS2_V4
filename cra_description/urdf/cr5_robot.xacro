        ixx="0"
        ixy="0"
        ixz="0"
        iyy="0"
        iyz="0"
        izz="0" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find cra_description)/meshes/cr5/base_link.dae" />
      </geometry>
      <material
        name="">
        <color rgba="0 0 0 1" />
