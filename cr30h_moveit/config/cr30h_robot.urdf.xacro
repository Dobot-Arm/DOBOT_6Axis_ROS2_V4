<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="cr30h_robot">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import cr30h_robot urdf file -->
    <xacro:include filename="$(find dobot_rviz)/urdf/cr30h_robot.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="cr30h_robot.ros2_control.xacro" />


    <xacro:cr30h_robot_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
